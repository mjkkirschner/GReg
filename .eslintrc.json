{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2015
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "camelcase": 0,
        "no-plusplus": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "prefer-destructuring": 0,
        "arrow-body-style": 0,
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ],
        "no-param-reassign": 0,
        "no-proto": 0,
        "no-restricted-syntax": 0,
        "consistent-return": 0,
        "max-len": [
            "error",
            {
                "code": 120,
                "comments": 120
            }
        ],
        "quotes": 0,
        "prefer-template": 0
    }
}